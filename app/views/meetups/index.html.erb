<h1>Listing meetups</h1>

<table>
  <tr>
    <th>Title</th>
    <th>User</th>
    <th>Begin at</th>
    <th>End at</th>
    <th>Public</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @meetups.each do |meetup| %>
  <tr>
    <td><%= meetup.title %></td>
    <td><%= meetup.user.name %></td>
    <td><%= meetup.begin_at %></td>
    <td><%= meetup.end_at %></td>
    <td><%= meetup.public %></td>
    <td><%= link_to 'Show', meetup %></td>
    <td><%= link_to 'Edit', edit_meetup_path(meetup) %></td>
    <td><%= link_to 'Destroy', meetup, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Meetup', new_meetup_path %>
